<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="goal.mapper.InputMyDataMapper">
	<resultMap type="goal.vo.FriendVO" id="friendVO">
		<id property="pgno" column="pgno"></id>
		<id property="myData" column="myData"></id>
		<id property="today" column="today"></id>
	</resultMap>
	
	<insert id="inputData" parameterType="goal.vo.InpuMyDataVO">
		insert into GK_INPUT_MYDATA values(#{pgno}, #{myData}, sysdate)
	</insert>
	
</mapper>